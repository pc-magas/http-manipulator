{
  "name": "http_manipulator",
  "version": "1.0.0",
  "description": "A simple reserve proxy and http server for mocking and manipulating http requests",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --verbose ./tests",
    "start": "node src/index.js",
    "dev_docker": "nodemon src/index.js",
    "debug_doker": "nodemon --inspect --inspect-brk=9595 src/index.js",
    "dev_local": "nodemon src/index.js --config=./config/config_local.json",
    "lint": "eslint"
  },
  "author": "Dimitrios Desyllas",
  "license": "MIT",
  "dependencies": {
    "arg": "^5.0.2",
    "better-sqlite3": "^8.0.1",
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.2",
    "busboy": "^1.6.0",
    "connect": "^3.7.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "gridjs": "^6.0.6",
    "jquery": "^3.6.1",
    "mime-types": "^2.1.35",
    "mmmagic": "^0.5.3",
    "nunjucks": "^3.2.3",
    "serve-static": "^1.15.0",
    "servicelocator": "^1.0.0",
    "urlrouter": "^0.5.4",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "eslint": "^8.33.0",
    "eslint-plugin-jsdoc": "^39.8.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testMatch": [
      "**/*.jest.js",
      "*.jest.js",
      "**/*.test.js",
      "*.test.js"
    ],
    "testTimeout": 70000
  }
}
